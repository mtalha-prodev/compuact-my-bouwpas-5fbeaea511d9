{
  "scripts": {
    "start": "expo start --dev-client",
    "start:production": "NODE_ENV=production & expo start --clear",
    "start:development": "NODE_ENV=development & expo start --clear",
    "start:test": "NODE_ENV=test & expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "build-preview-prod": "eas build --profile preview-prod",
    "build-preview-staging": "eas build --profile preview-staging",
